<!-- Fase 4: Cadastrando Usuários > Mensagens de alerta > Mensagens de erros aos usuários -->
{% if error %}
<div class="messages error">
    {{ error }}
</div>
{% endif %}

<!-- Fase 4: Controle de sessão do usuário > Atualizando usuários > Mensagem de sucesso -->
{% if success %}
<div class="messages success">
    {{ success }}
</div>
{% endif %}